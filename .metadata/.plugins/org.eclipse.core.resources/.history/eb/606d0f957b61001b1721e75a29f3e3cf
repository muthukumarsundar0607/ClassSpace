package programming_basics;

public class SumOfTwoNumber {
	

	
	/*Algorithm 
	 * Sum of Two Numbers 
	 * ------------------
	 * Declare two numbers 
	 * add two number 
	 * store the answer num 
	 * print the answer numbes*/

	
	public void sumofTwoMethod() {
		int a= 20 ; 
		int b= 30; 
		int d= a+b;
		System.out.println(d);
		
		
	}

	public static void main(String[] args) {
		SumOfTwoNumber ref = new SumOfTwoNumber();
		ref.sumofTwoMethod();
	}

}


